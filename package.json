{
  "name": "cypress-cycle-unit-test",
  "description": "Unit testing Cycle.js components in Cypress E2E test runner",
  "version": "0.0.0-development",
  "author": "Gleb Bahmutov <gleb.bahmutov@gmail.com>",
  "bugs": "https://github.com/bahmutov/cypress-cycle-unit-test/issues",
  "config": {
    "pre-git": {
      "commit-msg": "simple",
      "pre-commit": [
        "npm prune",
        "npm run deps",
        "npm test",
        "git add src/*.js",
        "npm run ban"
      ],
      "pre-push": [
        "echo disabled npm run secure",
        "npm run license",
        "npm run ban -- --all",
        "npm run size"
      ],
      "post-commit": [],
      "post-merge": []
    }
  },
  "engines": {
    "node": ">=6"
  },
  "files": [
    "src/*.js",
    "!src/*-spec.js"
  ],
  "homepage": "https://github.com/bahmutov/cypress-cycle-unit-test#readme",
  "keywords": [
    "cycle",
    "cypress",
    "cypress-io",
    "test",
    "testing"
  ],
  "license": "MIT",
  "main": "src/",
  "private": false,
  "publishConfig": {
    "registry": "http://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bahmutov/cypress-cycle-unit-test.git"
  },
  "scripts": {
    "ban": "ban",
    "deps": "deps-ok && dependency-check --no-dev .",
    "issues": "git-issues",
    "license": "license-checker --production --onlyunknown --csv",
    "lint": "standard --verbose --fix src/*.js cypress/integration/*.js",
    "prelint": "npm run pretty",
    "pretest": "npm run lint",
    "pretty": "prettier-standard 'src/*.js' 'cypress/integration/*.js'",
    "secure": "nsp check",
    "size": "t=\"$(npm pack .)\"; wc -c \"${t}\"; tar tvf \"${t}\"; rm \"${t}\";",
    "test": "npm run cy:run",
    "unused-deps": "dependency-check --unused --no-dev .",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release"
  },
  "release": {
    "analyzeCommits": "simple-commit-message"
  },
  "devDependencies": {
    "@bahmutov/add-typescript-to-cypress": "2.0.0",
    "@cycle/http": "14.9.0",
    "@cycle/isolate": "3.2.0",
    "@cycle/jsonp": "7.1.0",
    "@cycle/time": "0.12.0",
    "ban-sensitive-files": "1.9.2",
    "cypress": "3.2.0",
    "dependency-check": "3.3.0",
    "deps-ok": "1.2.2",
    "git-issues": "1.3.1",
    "immutable": "3.8.2",
    "license-checker": "16.0.0",
    "nsp": "3.2.1",
    "pre-git": "3.17.0",
    "prettier-standard": "8.0.0",
    "semantic-release": "14.0.3",
    "simple-commit-message": "4.0.3",
    "standard": "10.0.3",
    "travis-deploy-once": "4.4.1",
    "xstream": "11.2.0"
  },
  "dependencies": {
    "@cycle/dom": "20.1.0",
    "@cycle/run": "4.1.0"
  },
  "standard": {
    "globals": [
      "cy"
    ]
  }
}
